{
  "name": "perudo",
  "private": true,
  "version": "0.0.1",
  "description": "HFG version",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "angular": "^1.7.5",
    "angular-loader": "^1.7.5",
    "angular-route": "^1.7.5",
    "angular-resource": "^1.7.5",
    "angular-sanitize": "^1.7.5",
    "angular-websocket": "2.0.1",
    "angular-ui-bootstrap": "^2.5.6",
    "html5-boilerplate": "0.0.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "bootstrap": "^4.2.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.7.5",
    "cpx": "^1.5.0",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "protractor": "^5.4.1"
  },
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-loader,angular-route,angular-resource,angular-sanitize,angular-websocket/dist,angular-ui-bootstrap/dist,html5-boilerplate/dist,jquery/dist,bootstrap/dist}/**/*\" app/lib -C",
    "prestart": "npm install --experimental-modules",
    "start": "http-server -a localhost -p 8000 -c-1 ./app",
    "pretest": "npm install",
    "preupdate-webdriver": "npm install --experimental-modules",
    "update-webdriver": "webdriver-manager update --gecko false",
    "preprotractor": "npm run update-webdriver --experimental-modules"
  }
}
